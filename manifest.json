{
  "name": "Store My Tabs",
  "version": "1.0",
  "description": "Avoid tabs overwhelm, Store your tabs !",
  "browser_action": {
      "default_icon": "icon.png"
  },
    "icons": {
        "128": "icon.png",
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png"
       },
  "permissions": [
      "tabs", "http://*/*", "https://*/*", "contextMenus"
  ],
    "background_page": "background.html",
    "options_page": "options.html"
}
